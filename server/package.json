    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "express-ws": "^4.0.0",
    "http-status": "^1.4.2",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "throng": "^5.0.0",
    "touch": "^3.1.0",
    "uuid": "^8.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
